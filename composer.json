{
  "name": "manzadey/laravel-orchid-activity-log",
  "description": "Laravel Orchid Activity Log Wrapper",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Manzadey",
      "email": "andrey.manzadey@gmail.com",
      "homepage": "https://manzaday.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "manzadey/laravel-orchid-helpers": "^0.4.1",
    "spatie/laravel-activitylog": "^4.5"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Manzadey\\OrchidActivityLog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "stubs/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Manzadey\\OrchidActivityLog\\Providers\\FoundationServiceProvider",
        "Manzadey\\OrchidActivityLog\\Providers\\PlatformServiceProvider"
      ]
    }
  }
}
